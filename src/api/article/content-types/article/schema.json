{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "meta_description": {
      "type": "text"
    },
    "image_cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "keywords": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::keyword.keyword"
    },
    "content": {
      "type": "richtext"
    },
    "components": {
      "type": "dynamiczone",
      "components": [
        "blocks.content-conclusion",
        "blocks.product-description",
        "blocks.text-connection",
        "shared.seo",
        "blocks.product-table"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "articles"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "articles"
    },
    "content_ckeditor": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "articles"
    },
    "minsToRead": {
      "default": 6,
      "type": "integer"
    }
  }
}
